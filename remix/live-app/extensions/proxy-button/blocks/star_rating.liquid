<head></head>

<body>
  <div><button onclick="proxyReq()">PROXY REQUEST</button></div>
</body>

<script>
  async function proxyReq() {
    try {
      const response = fetch('https://test-youtubee.myshopify.com/apps/configs', {
        headers: {
          method: 'GET',
          'Content-Type': 'application/json',
        },
      });
      if (!response.ok) {
        throw new Error('Network response was not ok ' + response.statusText);
      }
      console.log(response, 'response');
      const data = await response.json();

      console.log(data, 'data');
    } catch (err) {
      console.log(err);
    }
  }
</script>

